{
    "name": "amorphine/cardpaygo-laravel",
    "description": "Cardpaygo payment system helper. Currently supports Hosted Form only",
    "type": "library",
    "require": {
        "illuminate/support": "~5.1|~5.2|~5.3|~5.4|~5.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Fedin",
            "email": "fe-pavel@yandex.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "Amorphine\\CardPayGo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amorphine\\CardPayGo\\Providers\\CardPayGoServiceProvider"
            ],
            "aliases": {
                "CardPayGo": "Amorphine\\CardPayGo\\Facades\\CardPayGo"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
